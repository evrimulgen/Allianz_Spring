		</profile>
		<profile>
			<id>profile-unmanaged-deployment</id>
			<distributionManagement>
				<repository>
					<id>repo.internal-release</id>
					<name>Internal TEST Release Repository</name>
					<url>https://sdlc.allianz.com.tr/nexus/content/repositories/internal-release/</url>
				</repository>
				<snapshotRepository>
