	<activeProfiles>
		<activeProfile>profile-unmanaged-build</activeProfile>
		<activeProfile>profile-thirdparty-build</activeProfile>
		<activeProfile>profile-common-build</activeProfile>
		<activeProfile>profile-ota-build</activeProfile>
		<activeProfile>profile-plugin-repositories</activeProfile>
	</activeProfiles>
	<mirrors>
		<mirror>
			<id>allianz-sdlc-central</id>
			<url>https://sdlc.allianz.com.tr/nexus/content/groups/thirdparty/</url>
